{
  "name": "svelte-proxied-store",
  "version": "1.0.0",
  "license": "MIT",
  "description": "svelte store for Object using Proxy to allow cybernetically enhanced magic.",
  "repository": "gitlab:clb1/svelte-proxied-store",
  "author": {
    "name": "Christophe Le Bars",
    "email": "<clb@toort.net>"
  },
  "main": "./dist/store.umd.js",
  "module": "./dist/store.es.js",
  "exports": {
    ".": {
      "import": "./dist/store.es.js",
      "require": "./dist/store.umd.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "prettier-standard --lint 'src/**/*.js'",
    "test": "mocha --require @babel/register",
    "build": "npm run clean && rollup -c"
  },
  "keywords": [
    "svelte",
    "store"
  ],
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/preset-env": "^7.16.5",
    "@babel/register": "^7.16.5",
    "rollup": "^2.62.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-dts": "^4.1.0",
    "assert": "^2.0.0",
    "lint-staged": "^12.1.4",
    "mocha": "^9.1.3",
    "prettier-standard": "^16.4.1",
    "release-it": "^14.11.8"
  },
  "lint-staged": {
    "*": [
      "prettier-standard --lint"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "release-it": {
    "gitlab": {
      "release": true
    }
  },
  "files": [
    "dist",
    "src"
  ]
}
