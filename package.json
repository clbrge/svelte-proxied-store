{
  "name": "svelte-proxied-store",
  "version": "2.0.3",
  "license": "MIT",
  "description": "Svelte store leveraging Proxy for advanced reactivity and seamless state management.",
  "repository": "clbrge/svelte-proxied-store",
  "author": {
    "name": "Christophe Le Bars",
    "email": "<clb@toort.net>"
  },
  "type": "module",
  "main": "./dist/store.umd.cjs",
  "module": "./dist/store.mjs",
  "types": "dist/store.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/store.mjs",
      "require": "./dist/store.umd.cjs",
      "types": "dist/store.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "prettier-standard --lint '{test,src}/**/*.js'",
    "test": "mocha",
    "build": "npm run clean && rollup -c"
  },
  "keywords": [
    "svelte",
    "store"
  ],
  "devDependencies": {
    "rollup": "^4.1.5",
    "rollup-plugin-dts": "^6.1.0",
    "assert": "^2.1.0",
    "lint-staged": "^15.0.2",
    "mocha": "^10.2.0",
    "prettier-standard": "^16.4.1",
    "release-it": "^16.2.1"
  },
  "lint-staged": {
    "*": [
      "prettier-standard --lint"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "files": [
    "dist",
    "src"
  ]
}
